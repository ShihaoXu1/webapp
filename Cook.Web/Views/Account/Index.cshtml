@model Cook.Web.ViewModels.Account.IndexViewModel

@{
    ViewBag.Title = "Index";
}

<h2>This is a user list </h2>

<ul>
    @if (Model.UserNames.Any())
    {
        foreach (var name in Model.UserNames)
        {
            <li>@name</li>
        }
    }
    else
    {
        <li>No Users in Database!</li>
    }
</ul>
<h2>This is a category list </h2>

<ul>
    @if (Model.Categories.Any())
    {
        foreach (var name in Model.Categories)
        {
            <li>@name</li>
        }
    }
    else
    {
        <li>No Categories in Database!</li>
    }
</ul>